json <- json-String / json-Integer / json-Objects
json-Objects <- json-Object-Comma* json-Object
json-Object-Comma <- json-Object ','
json-Object <- json-String / json-Integer /json-Pair / json-Map / json-Array
json-Map <- '{' json-Pair-Comma* json-Pair '}'
json-Pair-Comma <- json-Object ':' json-Object ','
json-Pair <- json-Object ':' json-Object
json-Array <- '[' json-Array-Element-Comma* json-Array-Element ']'
json-Array-Element <- json-Object
json-Array-Element-Comma <- json-Array-Element ','
json-Integer <- [0-9]+
json-String <- '\"' json-NotDquote* '\"'
json-NotDquote <- ! '\"' .
